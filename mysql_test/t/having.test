drop table if exists t;
create table t (c1 int, c2 int);
insert into t values (1,1), (2,2), (3,3), (1,3), (2,3);
select c1, c2 from t group by c2;
select count(c1), c2 from t group by c2;
select c1, c2 from t group by c2 having c2 > 1;
select count(c1), c2 from t group by c2 having count(c1) > 2;
select c1 as a from t group by c2 having a > 1;
select c1 as a from t group by c2 having count(a) > 2;
select c1 as a from t having a > 0;
select c1 as a from t having max(a) > 2;
--error ER_BAD_FIELD_ERROR
select c1 as a from t having c2 > 0;
--error ER_BAD_FIELD_ERROR
select c1 as a from t having b > 0;
--error ER_BAD_FIELD_ERROR
select c1 + 1 from t having c1 + 1 > 0;
select c2 from t group by c1 having c1 > 1;
--error ER_BAD_FIELD_ERROR
select c2 from t group by c2 having c1 > 1;
select * from t having c1 > 1;
select * from t group by c1 having c1 > 1;
select c1 as a from t group by c1 having a > 1;
select c1 as a from t group by a having c1 > 1;
select c1 as a from t group by a having a > 1;
--error ER_BAD_FIELD_ERROR
select c1 + c2 as a from t having c1 > 0;
select c1 + c2 as a from t having a > 0;
--error ER_BAD_FIELD_ERROR
select c1 + c2 as a from t having c1 + c2 > 0;
select c1 from t group by c2 + 1;
--error ER_BAD_FIELD_ERROR
select c1 from t group by c2 + 1 having c2 > 0;

--echo sqllogic/test/random/groupby test
drop table if exists tab0;
CREATE TABLE tab0(col0 INTEGER, col1 INTEGER, col2 INTEGER);
insert tab0 values(1,2,3),(2,1,3),(3,1,2),(4,5,1),(6,3,1);
select * from tab0;
SELECT - col2 AS col1 FROM tab0 GROUP BY col2, col1, col0 HAVING NOT + + col0 + - col1 >= + col0;
SELECT ALL col1 AS col2 FROM tab0 GROUP BY col1 HAVING NULL NOT BETWEEN ( + col1 + + col1 ) AND ( + col2 );
SELECT DISTINCT + + 61 col2 FROM tab0 WHERE NOT - 1 IS NULL GROUP BY col1 HAVING NOT - col1 >= col1;
SELECT DISTINCT AVG ( + - CAST( NULL AS SIGNED ) ) FROM tab0 GROUP BY col0, col1 HAVING + col1 > ( - col1 );
SELECT + AVG ( DISTINCT - - col1 ) * - + col1 * + 13 * - - col1 col0, + + 32 * COUNT( * ) + col1 IN ( + ( COUNT( * ) ) ) ccc FROM tab0;
SELECT + AVG ( DISTINCT - - col1 ) * - + col1 * + 13 * - - col1 col0, + + 32 * COUNT( * ) cc1, col1 IN ( + ( COUNT( * ) )) cc2 FROM tab0;
SELECT + AVG ( DISTINCT - - col1 ) * - + col1 * + 13 * - - col1 col0, + + 32 * COUNT( * ) + col1 IN ( + ( COUNT( * ) ) ) ccc FROM tab0 GROUP BY col1 HAVING + + 32 * COUNT( * ) + col1 IN ( + ( COUNT( * ) ) );
