DROP TABLE  IF EXISTS  t1;
CREATE TABLE t1 (
    c1 bigint NOT NULL AUTO_INCREMENT,
    c2 bigint, c3 bigint,
    c4 bigint NOT NULL DEFAULT 10,
    PRIMARY KEY (c1),
    UNIQUE INDEX c2_index (c2), 
    INDEX c3_index (c3));

show tables;
